<!DOCTYPE html>
<html lang="mk">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="Официјална веб страна на Семенарна СК; Плаќање на достава. Плаќање во готово. Адреса: Бул. Србија бр. 3, 1000 Скопје; Телефон: +389 (0)2 2783 225;">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>@ViewData["Title"] Семенарна | Наше семе, ваш успех!</title>

    <!-- lib css -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"
          integrity="sha512-iBBXm8fW90+nuLcSKlbmrPcLa0OT92xO1BIsZ+ywDWZCvqsWgccV3gFoRBv0z+8dLJgyAHIhR35VZc2oM/gI1w=="
          crossorigin="anonymous" />
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat&display=swap" rel="stylesheet">




    <!-- Css Styles internal lib -->
    <link rel="stylesheet" href="~/tp-lib/css/themify-icons.css" type="text/css">
    <link rel="stylesheet" href="~/tp-lib/css/elegant-icons.css" type="text/css">
    <link rel="stylesheet" href="~/tp-lib/css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="~/tp-lib/css/nice-select.css" type="text/css">
    <link rel="stylesheet" href="~/tp-lib/css/jquery-ui.min.css" type="text/css">
    <link rel="stylesheet" href="~/tp-lib/css/slicknav.min.css" type="text/css">
    <link rel="stylesheet" href="~/tp-lib/css/style.css" type="text/css">
    <link rel="stylesheet" href="~/tp-lib/css/nice-select.css" type="text/css">

    <!-- site css -->
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header class="header-section">
        <div class="header-top">
            <div class="container">
                <div class="ht-left">
                    <div class="mail-service">
                        <i class=" fa fa-envelope"></i>
                        info@semenarna.mk
                    </div>
                    <div class="phone-service">
                        <i class=" fa fa-phone"></i>
                        +389 (0)2 2783 225
                    </div>
                </div>
                <div class="ht-right">
                    @if (User.IsInRole("Admin"))
                    {
                        <a asp-area="Panel" asp-controller="Base" asp-action="Index" class="login-panel"><i class="fas fa-user-shield"></i>Admin Panel</a>
                    }
                    @if (User.Identity.IsAuthenticated)
                    {
                        <a asp-area="" asp-controller="Auth" asp-action="Logout" class="login-panel"><i class="fa fa-user"></i>Одјави се</a>
                    }
                    else
                    {
                        <a asp-area="" asp-controller="Auth" asp-action="Login" class="login-panel"><i class="fa fa-user"></i>Најави се</a>
                    }
                    <div class="lan-selector">
                        <select class="language_drop" name="countries" id="countries" style="width:300px;">
                            <option value='yt' data-image="/img/pride-flag.png" data-imagecss="flag yt"
                                    data-title="MK">
                                МK
                            </option>
                        </select>
                    </div>
                    <div class="top-social">
                        <a href="https://www.facebook.com/powerhousemkd/"><i class="fab fa-facebook-f"></i></a>
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="inner-header">
                <div class="row">
                    <div class="col-lg-3 col-md-3">
                        <div class="logo">
                            <a href="~/">
                                <img src="~/img/logo.png" alt="">
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6">
                        <div class="advanced-search">
                            <div class="input-group">
                                <input type="text" placeholder="Пребарај" id="store-search">
                                <button type="button"><i class="fas fa-search"></i></button>
                                <ul class="list-group" id="search-suggestions">
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 text-right col-md-3">
                        <ul class="nav-right">
                            <li class="cart-icon">
                                <a asp-area="Cart" asp-controller="Base" asp-action="Index">
                                    <i class="fas fa-shopping-bag"></i>
                                    <span id="cart-quantity-hover" style="opacity: 0;">0</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <div class="nav-item">
        <div class="container">
            <div class="nav-depart">
                <div class="depart-btn">
                    <i class="fas fa-bars"></i>
                    <span>Категории</span>
                    <ul class="depart-hover">
                        @foreach (var item in ViewBag.Categories) {
                            <li><a href="~/Store/Base/Category/1?category_id=@item.CategoryId">@item.Name</a></li>
                        }
                    </ul>
                </div>
            </div>
            <nav class="nav-menu mobile-menu">
                <ul>
                    <li><a asp-area="" asp-controller="Home" asp-action="Index">Почетна</a></li>
                    <li><a href="~/Store/Base/Index">Продавница</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Faq">ЧПП</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Contact">Контакт</a></li>
                </ul>
            </nav>
            <div id="mobile-menu-wrap"></div>
        </div>
    </div>
    <!-- Header End -->
    <div class="container-fluid pl-0 pr-0">
        <main role="main" class="pb-3">
            <div id="alert-container">
            </div>
            @RenderBody()
        </main>
    </div>
    <div class="partner-logo">
        <div class="container-lg">
            <div class="logo-carousel logo-section d-sm-block d-lg-flex align-items-lg-end justify-content-lg-between">
                <div class="logo-item mb-sm-3 mb-lg-0">
                    <div class="tablecell-inner brand-logo">
                        <img src="/img/brand-1.png" alt="">
                    </div>
                </div>
                <div class="logo-item mb-sm-3 mb-lg-0">
                    <div class="tablecell-inner brand-logo">
                        <img src="/img/brand-2.png" alt="">
                    </div>
                </div>
                <div class="logo-item mb-sm-2 mb-lg-0">
                    <div class="tablecell-inner brand-logo">
                        <img src="/img/brand-3.png" alt="">
                    </div>
                </div>
                <div class="logo-item mb-sm-3 mb-lg-0">
                    <div class="tablecell-inner brand-logo">
                        <img src="/img/brand-4.jpg" alt="">
                    </div>
                </div>
                <div class="logo-item mb-sm-3 mb-lg-0">
                    <div class="tablecell-inner brand-logo">
                        <img src="/img/brand-5.png" alt="">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <footer class="footer-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-3">
                    <div class="footer-left">
                        <div class="footer-logo">
                            <a href="#"><img src="~/tp-lib/img/footer-logo.png" alt=""></a>
                        </div>
                        <ul>
                            <li>Адреса: Бул. Србија бр. 3, 1000 Скопје</li>
                            <li>Телефон: +389 (0)2 2783 225</li>
                            <li>Email: info@semenarna.mk</li>
                        </ul>
                        <div class="footer-social">
                            <a href="#"><i class="fa fa-facebook"></i></a>
                            <a href="#"><i class="fa fa-instagram"></i></a>
                            <a href="#"><i class="fa fa-twitter"></i></a>
                            <a href="#"><i class="fa fa-pinterest"></i></a>
                        </div>
                    </div>
                </div>
                <div class="col-lg-2 offset-lg-1">
                    <div class="footer-widget">
                        <h5>Информации</h5>
                        <ul>
                            <li><a asp-area="" asp-controller="Home" asp-action="About">За нас</a></li>
                            <li><a asp-area="Cart" asp-controller="Base" asp-action="Checkout">Наплата</a></li>
                            <li><a asp-area="" asp-controller="Home" asp-action="Contact">Контакт</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-2">
                    <div class="footer-widget">
                        <h5>Профил</h5>
                        <ul>
                            <li><a asp-area="Cart" asp-controller="Base" asp-action="Index">Кошничка</a></li>
                            <li><a asp-area="Store" asp-controller="Base" asp-action="Index">Продавница</a></li>
                            <li><a asp-area="Cart" asp-controller="Base" asp-action="Checkout">Наплата</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="footer-widget">
                        <h5>Дополнително</h5>
                        <ul>
                            <li><a asp-area="" asp-controller="Home" asp-action="Faq">Често поставувани прашања</a></li>
                            <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Политика за приватност</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="copyright-reserved">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="copyright-text">
                        </div>
                        <div class="payment-pic">
                            <img src="~/tp-lib/img/payment-method.png" alt="">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js" integrity="sha512-qTXRIMyZIFb8iQcfjXWCO8+M5Tbc38Qi5WzdPOYZHIlZpzBHG3L3by84BBBOiRGiEb7KKtAOAs5qYdUiZiQNNQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="/tp-lib/js/jquery-ui.min.js"></script>
    <script src="/tp-lib/js/jquery.countdown.min.js"></script>
    <script src="/tp-lib/js/jquery.nice-select.min.js"></script>
    <script src="/tp-lib/js/jquery.zoom.min.js"></script>
    <script src="/tp-lib/js/jquery.dd.min.js"></script>
    <script src="/tp-lib/js/jquery.slicknav.js"></script>
    <script src="/tp-lib/js/owl.carousel.min.js"></script>
    <script src="/tp-lib/js/main.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
